	"metadata": {
		"title": "\nELSA-2011-0335:  tomcat6 security and bug fix update (IMPORTANT)\n",
		"affected": {
			"family": "unix",
			"platform": [
				"Oracle Linux 6"
			]
		},
		"reference": [
			{
