	"metadata": {
		"title": "\nELSA-2013-1674:  dracut security, bug fix, and enhancement update (MODERATE)\n",
		"affected": {
			"family": "unix",
			"platform": [
				"Oracle Linux 6"
			]
		},
		"reference": [
			{
				"source": "elsa",
