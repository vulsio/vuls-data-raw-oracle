	"metadata": {
		"title": "\nELSA-2020-3915:  libssh2 security update (MODERATE)\n",
		"affected": {
			"family": "unix",
			"platform": [
				"Oracle Linux 7"
			]
		},
		"reference": [
			{
				"source": "elsa",
