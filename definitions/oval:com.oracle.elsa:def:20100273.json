	"metadata": {
		"title": "\nELSA-2010-0273:  curl security, bug fix and enhancement update (MODERATE)\n",
		"affected": {
			"family": "unix",
			"platform": [
				"Oracle Linux 5"
			]
		},
		"reference": [
			{
