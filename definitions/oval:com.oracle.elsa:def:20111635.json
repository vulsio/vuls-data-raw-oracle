	"metadata": {
		"title": "\nELSA-2011-1635:  cups security and bug fix update (LOW)\n",
		"affected": {
			"family": "unix",
			"platform": [
				"Oracle Linux 6"
			]
		},
		"reference": [
			{
				"source": "elsa",
