	"metadata": {
		"title": "\nELSA-2011-1019:  libvirt security, bug fix, and enhancement update (MODERATE)\n",
		"affected": {
			"family": "unix",
			"platform": [
				"Oracle Linux 5"
			]
		},
		"reference": [
			{
				"source": "elsa",
				"ref_id": "ELSA-2011-1019",
