	"metadata": {
		"title": "\nELSA-2019-3600:  gnutls security, bug fix, and enhancement update (MODERATE)\n",
		"affected": {
			"family": "unix",
			"platform": [
				"Oracle Linux 8"
			]
		},
		"reference": [
			{
