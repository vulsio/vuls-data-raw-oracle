  "metadata": {
    "title": "\nELSA-2013-0581:  libxml2 security update (MODERATE)\n",
    "affected": {
      "family": "unix",
      "platform": [
        "Oracle Linux 5",
        "Oracle Linux 6"
      ]
    },
