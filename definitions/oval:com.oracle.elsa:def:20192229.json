	"metadata": {
		"title": "\nELSA-2019-2229:  spice-gtk security and bug fix update (MODERATE)\n",
		"affected": {
			"family": "unix",
			"platform": [
				"Oracle Linux 7"
			]
		},
		"reference": [
			{
