	"metadata": {
		"title": "\nELSA-2012-1061:  kernel security and bug fix update (MODERATE)\n",
		"affected": {
			"family": "unix",
			"platform": [
				"Oracle Linux 5"
			]
		},
		"reference": [
			{
				"source": "elsa",
