	"metadata": {
		"title": "\nELSA-2013-0519:  openssh security, bug fix and enhancement update (MODERATE)\n",
		"affected": {
			"family": "unix",
			"platform": [
				"Oracle Linux 6"
			]
		},
		"reference": [
