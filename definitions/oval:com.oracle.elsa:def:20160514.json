	"metadata": {
		"title": "\nELSA-2016-0514:  java-1.8.0-openjdk security update (IMPORTANT)\n",
		"affected": {
			"family": "unix",
			"platform": [
				"Oracle Linux 6"
			]
		},
		"reference": [
			{
