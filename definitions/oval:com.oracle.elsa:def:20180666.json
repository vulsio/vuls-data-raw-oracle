	"metadata": {
		"title": "\nELSA-2018-0666:  krb5 security, bug fix, and enhancement update (MODERATE)\n",
		"affected": {
			"family": "unix",
			"platform": [
				"Oracle Linux 7"
			]
		},
		"reference": [
			{
