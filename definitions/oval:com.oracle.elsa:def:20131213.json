	"metadata": {
		"title": "\nELSA-2013-1213:  gdm security update (IMPORTANT)\n",
		"affected": {
			"family": "unix",
			"platform": [
				"Oracle Linux 5"
			]
		},
		"reference": [
			{
				"source": "elsa",
