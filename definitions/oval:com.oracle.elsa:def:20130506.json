	"metadata": {
		"title": "\nELSA-2013-0506:  samba4 security, bug fix and enhancement update (MODERATE)\n",
		"affected": {
			"family": "unix",
			"platform": [
				"Oracle Linux 6"
			]
		},
		"reference": [
			{
				"source": "elsa",
				"ref_id": "ELSA-2013-0506",
