	"metadata": {
		"title": "\nELSA-2023-5736:  java-11-openjdk security and bug fix update (MODERATE)\n",
		"affected": {
			"family": "unix",
			"platform": [
				"Oracle Linux 7"
			]
		},
		"reference": [
			{
				"source": "elsa",
