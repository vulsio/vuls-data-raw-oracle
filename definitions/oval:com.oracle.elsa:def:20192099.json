	"metadata": {
		"title": "\nELSA-2019-2099:  samba security, bug fix, and enhancement update (MODERATE)\n",
		"affected": {
			"family": "unix",
			"platform": [
				"Oracle Linux 7"
			]
		},
		"reference": [
			{
