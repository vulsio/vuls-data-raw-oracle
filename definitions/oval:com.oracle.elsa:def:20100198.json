	"metadata": {
		"title": "\nELSA-2010-0198:  openldap security and bug fix update (MODERATE)\n",
		"affected": {
			"family": "unix",
			"platform": [
				"Oracle Linux 5"
			]
		},
		"reference": [
			{
				"source": "elsa",
