  "metadata": {
    "title": "\nELSA-2020-5961:  libvirt security update (IMPORTANT)\n",
    "affected": {
      "family": "unix",
      "platform": [
        "Oracle Linux 7"
      ]
    },
    "reference": [
      {
        "source": "elsa",
        "ref_id": "ELSA-2020-5961",
